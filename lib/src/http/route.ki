
class Route[T] {
	public handler: T;
	public args: Array[RouteArg];

	public fn params(path: *String) Map[String] {
		let result = Map[String]{};
		let parts = path.split("/"); 
		each this.args as arg {
			let p = parts.get(arg.i) !! continue;;
			result.set(arg.name.copy(), p + "");
		}
		return result;
	}
}
